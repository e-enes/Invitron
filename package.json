{
  "name": "sene-bot",
  "version": "2.0.0",
  "description": "A powerful discord bot that helps you keep track of your server's invite count",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node .",
    "build": "node scripts/build.js",
    "dev": "nodemon",
    "format": "prettier . --check",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "prepare": "husky"
  },
  "author": "Enes Tahiri",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src",
      ".env",
      "scripts"
    ],
    "ext": "ts",
    "exec": "pnpm build && pnpm start",
    "delay": 2500,
    "env": {
      "NODE_ENV": "development"
    }
  },
  "homepage": "https://github.com/e-enes/discord-invite-tracker#readme",
  "bugs": {
    "email": "contact@enes.gg",
    "url": "https://github.com/e-enes/discord-invite-tracker/issues"
  },
  "packageManager": "pnpm@8.15.3",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "discord-api-types": "^0.37.74",
    "discord.js": "^14.14.1",
    "fs": "0.0.1-security",
    "glob": "^10.3.10",
    "i18next": "^23.10.1",
    "i18next-fs-backend": "^2.3.1",
    "mysql2": "^3.9.2",
    "sqlite3": "^5.1.7"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  }
}
