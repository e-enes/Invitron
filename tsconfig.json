{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "traceResolution": true,
    "noImplicitOverride": true
  },
  "include": ["src/**/*.ts", "*.js"],
  "exclude": ["dist", "node_modules"]
}
